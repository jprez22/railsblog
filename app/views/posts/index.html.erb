<h1 class="title">Blog Index<h1>


<section class ="section">
	<div class="container">
		<% @posts.each do |post| %>
			<div class="card">
				<div class="card-body">
					<h5 class="card-title"><%= link_to post.title, post %></h5>
					<p class="h6"> <%= post.content %></p>
					<!-- <a href="#" class="btn btn-primary">Go somewhere</a> -->
				</div>
			</div>
		<% end %>
	</div>
</section>

	


